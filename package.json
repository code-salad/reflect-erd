{
  "name": "vsequel",
  "version": "0.1.2",
  "description": "A CLI tool for extracting database schemas and generating ERD diagrams from PostgreSQL and MySQL databases",
  "keywords": [
    "erd",
    "database",
    "schema",
    "plantuml",
    "postgresql",
    "mysql",
    "cli"
  ],
  "homepage": "https://github.com/reflect/erd#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reflect/erd.git"
  },
  "scripts": {
    "encrypt": "dotenvx encrypt",
    "decrypt": "dotenvx decrypt",
    "format": "bunx ultracite format",
    "lint": "bunx ultracite lint",
    "seed": "bun run seeds/setup.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:docker": "docker compose -f tests/docker-compose.yml up -d --wait && bun run seeds/setup.ts && bun test docker-integration.spec.ts && docker compose -f tests/docker-compose.yml down"
  },
  "license": "MIT",
  "author": "Reflect",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "bin": {
    "vsequel": "src/cli/index.ts"
  },
  "files": [
    "src/**/*.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "mysql2": "^3.14.3",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "zod": "^4.0.15",
    "@dotenvx/dotenvx": "^1.48.4",
    "@biomejs/biome": "2.1.2",
    "@types/bun": "latest",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.11",
    "lefthook": "^1.12.2",
    "ultracite": "5.1.2"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  }
}
