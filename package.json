{
  "name": "vsequel",
  "version": "0.1.4",
  "description": "A CLI tool for extracting database schemas and generating ERD diagrams from PostgreSQL and MySQL databases",
  "keywords": [
    "erd",
    "database",
    "schema",
    "plantuml",
    "postgresql",
    "mysql",
    "cli"
  ],
  "homepage": "https://github.com/reflect/erd#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reflect/erd.git"
  },
  "scripts": {
    "encrypt": "dotenvx encrypt",
    "decrypt": "dotenvx decrypt",
    "format": "npx ultracite format",
    "lint": "npx ultracite lint",
    "seed": "npx tsx seeds/setup.ts",
    "build": "rm -rf dist && npx tsc",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:watch": "npx test --watch",
    "test:docker": "docker compose -f tests/docker-compose.yml up -d --wait && npx tsx seeds/setup.ts && npm run vitest ./tests/docker-integration.spec.ts && docker compose -f tests/docker-compose.yml down"
  },
  "license": "MIT",
  "author": "Reflect",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "vsequel": "dist/cli/index.js"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.js.map",
    "src/**/*.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "mysql2": "^3.14.3",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@dotenvx/dotenvx": "^1.48.4",
    "@types/node": "^24.2.1",
    "lefthook": "^1.12.2",
    "tsx": "^4.20.5",
    "ultracite": "5.1.2",
    "vitest": "^3.2.4",
    "zod": "^3.24.1"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  }
}
